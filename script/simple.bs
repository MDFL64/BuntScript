fn alpha(a: number, b: number, c: bool) -> number {
    a + b - (if c { 10 } else { 20 } + 62)
}

fn beta(a: number, b: number, c: bool) -> number {
    a * b + 234.5
}

fn delta(a: number, b: number, c: bool) -> number {
    a / b + 1e4
}

fn gamma(a: number, b: number, c: bool) -> number {
    if c == true {
        a + a + a + a + 1e-4
    } else {
        b * b
    }
}
